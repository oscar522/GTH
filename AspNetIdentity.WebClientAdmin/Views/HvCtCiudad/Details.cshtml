@model AspNetIdentity.Models.HvCtCiudadModel

@{
    ViewBag.Title = "Detalle Ciudad" + @Html.DisplayFor(modelItem => Model.NOMBRE);
}

<div class="card mb-4 wow fadeIn">

    <!--Card content-->
    <div class="card-body d-sm-flex justify-content-between">
        <h3 class="card-title">Detalle Ciudad  @Html.DisplayFor(modelItem => Model.NOMBRE) </h3>
        <p class="card-text">Hoja de Vida / Ciudad</p>
    </div>

</div>
<div class="row wow fadeIn">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">  </div>
            <div class="card-body">
                
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.NOMBRE_PAIS)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NOMBRE_PAIS)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.NOMBRE_DEPTO)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NOMBRE_DEPTO)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.NOMBRE)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NOMBRE)
                    </dd>

                </dl>
            </div>
        </div>
    </div>
</div>
