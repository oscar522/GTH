@model IEnumerable<AspNetIdentity.Models.HvCtTitulosModel>

@{
    ViewBag.Title = "Index";
}
@section Scripts
             {
    @Scripts.Render("~/bundles/jqueryval")   @Scripts.Render("~/Scripts/ModalControl.js")

    <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
    <script src="http://code.jquery.com/ui/1.11.0/jquery-ui.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
}
<div class="card mb-4 wow fadeIn">
    <!--Card content-->
    <div class="card-body d-sm-flex justify-content-between">
        <h3 class="card-title">Titulos</h3>
        <p class="card-text">Hoja de Vida / Titulos</p>
    </div>
</div>
<div class="row wow fadeIn">
    <div class="col-md-12">
        <div class="card">

            <div class="card-header">  <a href="#" onclick="Ejecutar ('','HvCtTitulos','Crear'); return false;">Crear Titulos </a></div>
            <div class="card-body">
                <table class="table">
                    <tr>

                        <th>
                            @Html.DisplayNameFor(model => model.NOMBRE_PAIS)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.NOMBRE_DEPTO)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.NOMBRE_CIUDAD)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.NOMBRE)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.TIPO)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>

                            <td>
                                @Html.DisplayFor(modelItem => item.NOMBRE_PAIS)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.NOMBRE_DEPTO)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.NOMBRE_CIUDAD)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.NOMBRE)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.TIPO)
                            </td>
                            <td>
                                <a href="#" onclick="Ejecutar ('@item.ID_CT_TITULOS','HvCtTitulos','Edit'); return false;" title="Modificar" style="color: #013ADF;"><i class="fa fa-refresh" aria-hidden="true"></i></a> |
                                <a href="#" onclick="Ejecutar ('@item.ID_CT_TITULOS','HvCtTitulos','Detail'); return false;" title="Detalle" style="color: #013ADF;"><i class="fa fa-search" aria-hidden="true"></i> </a> |
                                <a href="#" onclick="Ejecutar ('@item.ID_CT_TITULOS','HvCtTitulos','Delete'); return false;" title="Borrar" style="color: #013ADF;"><i class="fa fa-trash" aria-hidden="true"></i></a> |

                            </td>
                        </tr>
                    }

                </table>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="centralModalLGInfoDemo" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-lg modal-notify modal-info" role="document">
        <!--Content-->
        <div class="modal-content">
            <!--Header-->
            <div class="modal-header">
                <p class="heading lead">Modal Info</p>

                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="white-text">×</span>
                </button>
            </div>
            <!--Body-->
            <div class="modal-body">
            </div>
            <!--Footer-->
            <div class="modal-footer">
                <a type="button" class="btn btn-outline-info waves-effect" data-dismiss="modal" onclick="location.reload()">Cerrar</a>
            </div>
        </div>
        <!--/.Content-->
    </div>
</div>
<!-- Modal: modalCart -->
