@model AspNetIdentity.Models.PerfilCtIdiomasModel

@{
    ViewBag.Title = "Delete";
}

<!--Card content-->
<div class="card mb-4 wow fadeIn">
    <div class="card-body d-sm-flex justify-content-between">
        <h3 class="card-title">Detalle Idioma  @Html.DisplayFor(modelItem => Model.NOMBRE) </h3>
        <p class="card-text">Perfil  /Idioma</p>
    </div>
</div>
<div class="row wow fadeIn">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">  </div>
            <div class="card-body">
                <hr />
                <dl class="dl-horizontal">
                   
                    <dt>
                        @Html.DisplayNameFor(model => model.NOMBRE)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NOMBRE)
                    </dd>
                   
                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Borrar" class="btn btn-default" /> |
                       
                    </div>
                }
            </div>
        </div>
    </div>
</div>