@model IEnumerable<AspNetIdentity.Models.HvUbicacionModel>

<table class="table" >
    <tr>

        <th width="16%">
            PAIS
        </th>
        <th width="16%">
            DEPARTAMENTO
        </th>
        <th>
            CIUDAD
        </th>
        <th width="16%">
            @Html.DisplayNameFor(model => model.NOMENCLATURA)
        </th>
        <th width="16%">
            @Html.DisplayNameFor(model => model.LOCALIDAD)
        </th>
        <th width="10%"></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.NOMBRE_PAIS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOMBRE_DEPTO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOMBRE_CIUDAD)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.NOMENCLATURA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LOCALIDAD)
            </td>
            <td>
                <a href="#" onclick="Ejecutar ('@item.ID_HV_HUBICACION','HvUbicacion','Edit'); return false;" title="Modificar" style="color: #013ADF;"><i class="fa fa-refresh" aria-hidden="true"></i></a> |
                <a href="#" onclick="Ejecutar ('@item.ID_HV_HUBICACION','HvUbicacion','Details'); return false;" title="Detalle" style="color: #013ADF;"><i class="fa fa-search" aria-hidden="true"></i> </a> |
                <a href="#" onclick="Ejecutar ('@item.ID_HV_HUBICACION','HvUbicacion','Delete'); return false;" title="Borrar" style="color: #013ADF;"><i class="fa fa-trash" aria-hidden="true"></i></a> |

            </td>
        </tr>
    }
</table>
               