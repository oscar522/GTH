@model AspNetIdentity.Models.PerfilProcesosModel

@{
    ViewBag.Title = "Borrar Finalidad" + @Html.DisplayFor(modelItem => Model.NOMBRE);
}

<div class="card mb-4 wow fadeIn">

    <!--Card content-->
    <div class="card-body d-sm-flex justify-content-between">
        <h3 class="card-title">Borrar Proceso  @Html.DisplayFor(modelItem => Model.NOMBRE) </h3>
        <p class="card-text">Perfil / Proceso</p>
    </div>

</div>
<div class="row wow fadeIn">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">  </div>
            <div class="card-body">
                <dl class="dl-horizontal">
                    
                    <dt>
                        @Html.DisplayNameFor(model => model.NOMBRE)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NOMBRE)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.DESCRIPCION)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.DESCRIPCION)
                    </dd>

                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Delete" class="btn btn-default" /> |
                      
                    </div>
                }
            </div>
        </div>
    </div>
</div>


